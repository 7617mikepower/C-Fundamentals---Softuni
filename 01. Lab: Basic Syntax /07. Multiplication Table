//You will receive an integer as input from the console.
//Print the 10 times table for this integer.

#include <iostream>

using namespace std;

int main()

{
    int number;

    cin >> number;

    for (size_t i {1}; i <= 10; i++)
    {
        cout << number << " X " << i << " = " << number *i << endl;
    }

    return 0;
}

