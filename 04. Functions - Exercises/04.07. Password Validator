//Write a program that find the count of the vowels contained in the text.
//48-57 = 0-9
//65-90 + 32 = a,b,c...

#include <iostream>
#include<string>
using namespace std;

bool checkNumber(char a)
{
    for(int i = 48; i<58; i++)
    {
        if (a == i)
        {
            return true;
        }
    }
    return false;
}

bool checkLetters(char a)
{
    for(int i = 97; i<123; i++)
    {
        if (a == i)
        {
            return true;
        }
    }
    return false;
}

bool checkCapLetters(char a)
{
    for(int i = 65; i<91; i++)
    {
        if (a == i)
        {
            return true;
        }
    }
    return false;
}

int main()
{
    string text;
    cin >> text ;

    bool symbol;
    int noSymbol = 0;
    int sumNumbers = 0;
    int sumLetters = 0;
    int sumCapLetters = 0;


    if (text.size() < 6 || text.size() > 10)
    {
        cout << "Password must be between 6 and 10 characters" << endl;
    }

    for (size_t i = 0; i<text.size() ; i++)
    {
        symbol = checkNumber(text[i]);
        if (symbol)
        {
            sumNumbers ++;
        }
        else noSymbol++;
        symbol = checkLetters(text[i]);
        if (symbol)
        {
            sumLetters ++;
        }
        else noSymbol++;
        symbol = checkCapLetters(text[i]);
        if (symbol)
        {
            sumCapLetters ++;
        }
        else noSymbol++;
    }

    if (noSymbol > text.size()*2)
    {
        cout << "Password must consist only of letters and digits" << endl;
    }
    if(sumNumbers < 2)
    {
        cout << "Password must have at least 2 digits" << endl;
    }
    if (sumNumbers >= 2 && noSymbol <= text.size()*2 && (text.size() >= 6 && text.size() <= 10)){
        cout << "Password is valid" << endl;
    }
    return 0;
}
